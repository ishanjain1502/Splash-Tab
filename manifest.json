{
    "manifest_version": 2,
    "name": "splahTab",
    "version": "1.0.0",
    "description": "Beutiful photos from unsplash everytime you open  a new tab",
    "icons": {
        "16": "icons/splash.png",
        "32": "icons/splash.png",
        "48": "icons/splash.png",
        "64": "icons/splash.png",
        "128": "icons/splash.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "minimum_chrome_version": "60"
}